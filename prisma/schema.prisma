// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Organizer {
  id        Int      @id @default(autoincrement())
  name      String
  manager   String?
  email     String
  password  String
  phone     String?
  address   String?
  about     String?
  url       String?
  sosmed    Json?
  createdAt DateTime @default(now())
}
